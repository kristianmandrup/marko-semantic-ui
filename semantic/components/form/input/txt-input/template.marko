
<div body-only-if="!data.fieldWrap" class="$data.field field">
  <label if="data.label">$data.label</label>
  <div body-only-if="!data.ui" class="ui $data.ui input">
    <input type="$data.type" value="$data.value" name="$data.name" placeholder="$data.placeholder" tab-index="$data.tab">
  </div>
</div>